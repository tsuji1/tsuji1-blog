{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "types": [
            "@cloudflare/workers-types",
            "./types.d.ts"
        ],
        "lib": [
            "es2022"
        ],
        "module": "esnext",
        "target": "es2022",
        "moduleResolution": "bundler",
        "isolatedModules": true,
        "noEmit": true
    },
    "include": [
        "./**/*.ts"
    ]
}
